<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Truthing Interface</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ImageCaptionServlet</servlet-name>
    <servlet-class>ImageCaptionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageCaptionServlet</servlet-name>
    <url-pattern>/ImageCaptionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ImageCaptionDownloadServlet</servlet-name>
    <servlet-class>ImageCaptionDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageCaptionDownloadServlet</servlet-name>
    <url-pattern>/ImageCaptionDownloadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>UserLogin.jsp</welcome-file>
  </welcome-file-list>
</web-app>